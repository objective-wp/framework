{
  "name": "objective-wp/framework",
  "keywords": ["objective", "wordpress", "framework", "objective-wp"],
  "type": "library",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/objective-wp/framework/issues",
    "source": "https://github.com/objective-wp/framework"
  },
  "authors": [
    {
      "name": "Jason Gallavin",
      "email": "jasongallavin@gmail.com"
    }
  ],
  "require": {
    "php-di/php-di": "^5.4",
    "mockery/mockery": "^0.9.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "ObjectiveWP\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ObjectiveWP\\Framework\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "npm i"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
